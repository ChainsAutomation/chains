amqp:
  exchange: chains
  host: localhost
  password: guest
  port: '5672'
  ssl: '0'
  user: guest
main:
  bindir: /srv/chains/bin
  confdir: /etc/chains
  datadir: /srv/chains/data
  heartbeat: '5'
  libdir: /srv/chains/lib/chains
